<fieldset class="transaction">
<legend>Transaction ID:  <%= order_transaction.id %></legend>
<ol>
<li>
  action:
  <%= order_transaction.action %>
</li>
<li>
  amount:
  <%= order_transaction.amount %>
</li>
<li>
  response_success:
  <%= order_transaction.response_success %>
    <% if order_transaction.response_success %>
    <%= image_tag("Badge-tick.png", :size => "16x16") %>
  <% end %>
</li>
<li>
  response_authorization:
  <%= order_transaction.response_authorization %>

</li>
<li>
  response_message:
  <%= order_transaction.response_message %>
</li>
<li>
  trxn_status:
  <%= order_transaction.trxn_status %>
</li>
<li>
  trxn_number:
  <%= order_transaction.trxn_number %>
</li>
<li>
  trxn_reference:
  <%= order_transaction.trxn_reference %>
</li>
<li>
  trxn_option_1:
  <%= order_transaction.trxn_option_1 %>
</li>
<li>
  trxn_option_2:
  <%= order_transaction.trxn_option_2 %>
</li>
<li>
  trxn_option_3:
  <%= order_transaction.trxn_option_3 %>
</li>
<li>
  auth_code:
  <%= order_transaction.auth_code %>
</li>
<li>
  return_amount:
  <%= order_transaction.return_amount %>
</li>
<li>
  trxn_error:
  <%= order_transaction.trxn_error %>
</li>
<li>
  params:
  <%= order_transaction.params %>
</li>



<li>
  Created at:
  <%= order_transaction.created_at %>
</li>
<li>
  Updated at:
  <%= order_transaction.updated_at %>
</li>
</ol>
</fieldset>


